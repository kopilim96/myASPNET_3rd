@model IEnumerable<StudentManagement.Models.SchoolViewModel.EnrollmentDateGroup> 
@{
	ViewData["Title"] = "Student Statistics";
}

<h2>Student Statistics</h2>
<table>
	<tr>
		<th>Enrollment Date</th>
		<th>Students</th>
	</tr>
	@foreach (var item in Model) { 
		<tr>
			<td>@Html.DisplayFor(m => item.EnrollmentDate)</td>
			<td>@item.studentCount</td>
		</tr>
	}
</table>